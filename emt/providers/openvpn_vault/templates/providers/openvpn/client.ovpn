client

tls-client
auth SHA256
cipher AES-256-CBC
remote-cert-tls server
tls-version-min 1.2

proto udp
remote {{ openvpn_host }} {{ openvpn_port }}
dev tun

resolv-retry 5
nobind
keepalive 5 30
compress lzo
persist-key
persist-tun
verb 3



route-method exe
route-delay 2

key-direction 1

<ca>
{{ issuing_ca }}
</ca>

<tls-auth>
{{ tls_auth }}
</tls-auth>

<cert>
{{ certificate }}
</cert>

<key>
{{ private_key }}
</key>
